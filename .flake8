[flake8]
# Make flake8 compatible with black
max-line-length = 88
extend-ignore = 
    # E203: whitespace before ':' (conflicts with black)
    E203,
    # W503: line break before binary operator (conflicts with black)
    W503
exclude = 
    .git,
    __pycache__,
    .venv,
    venv,
    .env,
    logs,
    *.egg-info